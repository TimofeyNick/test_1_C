#include <iostream>
#include <string>
#include <math.h>

int get_min(int a, int b, int c){
    // переписать функцию, она НЕВЕРНА
    if (a == -1 && b == -1){return -1;}
    else if (a == -1 && b != -1){return b;}
    else if (b == -1 && a != -1){return a;}
    else{
        if (a <= b){return a;}
        else{return b;}
    }
}

int main() {
    int size = 0;
    int ver_1 = 0, ver_2 =0;
    std:: cin >> size >> ver_1 >> ver_2;
    ver_1 = ver_1 - 1;
    ver_2 = ver_2 - 1;

   int A[size+2][size+1][size+1];
   for(int i=0; i < size; i++){
       for(int j=0; j < size; j++){
           std:: cin >> A[0][i][j];
       }
   }
   for(int k = 1; k < size + 1; k++){
       for(int i=0; i< size; i++){
           for(int j=0; j< size; j++){
               A[k][i][j] = get_min(A[k-1][i][j], A[k-1][i][k], A[k-1][k][j]);
           }

       }
   }
   std:: cout << A[4][ver_1][ver_2];
}
